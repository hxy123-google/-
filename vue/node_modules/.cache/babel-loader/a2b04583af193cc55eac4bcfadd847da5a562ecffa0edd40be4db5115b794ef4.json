sole.log(this.pro);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pro","pageNum","pageSize","total","journal","author","type","user","JSON","parse","localStorage","getItem","pageNum1","pageSize1","total1","mounted","loadPro","methods","load","$request","get","params","role","then","res","userData","list","console","log","startDate","i","length","dateTimeString","time","dateTime","Date","year","getFullYear","month","getMonth","day","getDate","id","code","$message","error","msg"],"sources":["src/views/front/Professor.vue"],"sourcesContent":["<template>\r\n    <div class=\"main-content\">\r\n        <div style=\"display: flex; grid-gap:10px\">\r\n            <div style=\"flex:1; width:150px\" class=\"card\">\r\n                <div style=\"height:300px\" class=\"search\">\r\n                    <div class=\"left\">\r\n                        <div style=\"margin: 15px; text-align: center\">\r\n                            <el-image :src=\"pro.avatar\"></el-image>\r\n                        </div>\r\n\r\n                    </div>\r\n                    <div class=\"right\">右侧内容</div>\r\n                </div>\r\n                <div class=\"table\">\r\n                    <el-table :data=\"tableData\">\r\n                        <el-table-column prop=\"id\" label=\"序号\" width=\"80\" align=\"center\" sortable></el-table-column>\r\n                        <el-table-column prop=\"img\" label=\"文献封面\" show-overflow-tooltip>\r\n                            <template v-slot=\"scope\">\r\n                                <div style=\"display: flex; align-items: center\">\r\n                                    <el-image style=\"width: 60px; height: 40px; border-radius: 10px\"\r\n                                        v-if=\"scope.row.img\" :src=\"scope.row.img\"\r\n                                        :preview-src-list=\"[scope.row.img]\"></el-image>\r\n                                </div>\r\n                            </template>\r\n                        </el-table-column>\r\n                        <el-table-column prop=\"name\" label=\"文献名称\" show-overflow-tooltip width=\"300\">\r\n                            <template v-slot=\"scope\">\r\n                                <a :href=\"'/front/articleDetail?id=' + scope.row.id\">{{ scope.row.name }}</a>\r\n                            </template>\r\n                        </el-table-column>\r\n\r\n                        <el-table-column prop=\"author\" label=\"文献作者\" show-overflow-tooltip></el-table-column>\r\n                        <el-table-column prop=\"content\" label=\"综述\" show-overflow-tooltip></el-table-column>\r\n                        <el-table-column prop=\"type\" label=\"文献类别\">\r\n                            <template v-slot=\"scope\">\r\n                                <span v-if=\"scope.row.type === 'CHINESE'\" style=\"color: #b67259\">中文 </span>\r\n                                <span v-else style=\"color: #448231\">英文</span>\r\n                            </template>\r\n                        </el-table-column>\r\n                        <el-table-column prop=\"price\" label=\"所属积分\">\r\n                            <template v-slot=\"scope\">\r\n                                <span style=\"color: #12b127; font-size: 15px\" v-if=\"scope.row.price > 0\">{{\r\n                                    scope.row.price }} 积分</span>\r\n                                <span v-else style=\"color: green\">公开资料</span>\r\n                            </template>\r\n                        </el-table-column>\r\n                        <!-- <el-table-column prop=\"file\" label=\"文献链接\" show-overflow-tooltip>\r\n                            <template v-slot=\"scope\">\r\n                                <el-button type=\"warning\" size=\"mini\" @click=\"down(scope.row.file)\">点击下载</el-button>\r\n                            </template> -->\r\n                        <!-- </el-table-column> -->\r\n                        <el-table-column prop=\"discount\" label=\"文献折扣\">\r\n                            <template v-slot=\"scope\">\r\n                                <span style=\"color: #448231\" v-if=\"scope.row.discount < 1\">{{ scope.row.discount * 10 }}\r\n                                    折</span>\r\n                                <span style=\"color: #448231\" v-else>——</span>\r\n                            </template>\r\n                        </el-table-column>\r\n                        <!-- <el-table-column prop=\"file\" label=\"文献资料\" show-overflow-tooltip></el-table-column> -->\r\n                        <!-- <el-table-column prop=\"discount\" label=\"文献折扣\"></el-table-column> -->\r\n                        <el-table-column prop=\"reference\" label=\"文献引用量\"></el-table-column>\r\n                        <el-table-column prop=\"time\" label=\"发表时间\"></el-table-column>\r\n                    </el-table>\r\n\r\n                    <div class=\"pagination\">\r\n                        <el-pagination background @current-change=\"handleCurrentChange\" :current-page=\"pageNum\"\r\n                            :page-sizes=\"[5, 10, 20]\" :page-size=\"pageSize\" layout=\"total, prev, pager, next\"\r\n                            :total=\"total\">\r\n                        </el-pagination>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div style=\"width:400px\" class=\"card\">\r\n                <div v-for=\"item in userData\" :key=\"item.id\">\r\n                    <div>\r\n                        <div style=\"font-size: 18px; margin: 10px 0\">头像</div>\r\n                        <div>\r\n                            <img :src=\"item.avatar\" controls style=\"width: 65%; height: 400px\"\r\n                                @click=\"naveToUser(item.id)\">\r\n                            <!-- <div style=\"margin-top: 10px\">头像：<a :href=\"courseData.file\" target=\"_blank\">{{ userData.avatar }}</a></div> -->\r\n                        </div>\r\n                        <!-- <div style=\"font-size: 18px; margin: 10px 0\">邮箱</div>\r\n                        <div>{{ userData.email }}</div>\r\n                        <div style=\"font-size: 18px; margin: 10px 0\">电话</div>\r\n                        <div>{{ userData.phone }}</div> -->\r\n                    </div>\r\n                    <!--   课程介绍区域   -->\r\n                    <div style=\"margin-top: 20px\">\r\n                        <div style=\"font-size: 18px\">个人介绍</div>\r\n                        <div style=\"margin-top: 10px\" v-html=\"item.introduce\" class=\"w-e-text w-e-text-container\">\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"pagination\">\r\n                    <el-pagination background @current-change=\"handleCurrentChange1\" :current-page=\"pageNum1\"\r\n                        :page-sizes=\"[5, 10, 20]\" :page-size=\"pageSize1\" layout=\"total, prev, pager, next\"\r\n                        :total=\"total1\">\r\n                    </el-pagination>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n    </div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n    name: \"Professor\",\r\n    data() {\r\n        return {\r\n            tableData:[],  // 所有的数据\r\n            pro:null,\r\n            pageNum: 1,   // 当前的页码\r\n            pageSize: 8,  // 每页显示的个数\r\n            total: 0,\r\n            name: null,\r\n            journal: null,\r\n            author: null,\r\n            type: null,\r\n            user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n            pageNum1: 1,   // 当前的页码\r\n            pageSize1: 8,\r\n            total1: 0,\r\n        }\r\n    },\r\n    mounted() {\r\n        this.loadPro(2);\r\n    },\r\n\r\n    methods: {\r\n        load(pageNum) {\r\n            if (this.author !== null) {\r\n                this.$request.get('/user/getAccPro',\r\n                    {\r\n                        params: {\r\n                            name: this.author,\r\n                            role: 'PRO',\r\n                            pageNum1: this.pageNum1,\r\n                            pageSize1: this.pageSize1,\r\n                        }\r\n                    }\r\n                ).then(res => {\r\n                    this.userData = res.data?.list\r\n                    this.total1 = res.data?.total\r\n\r\n                    console.log(this.userData);\r\n                })\r\n            }\r\n            if (pageNum) this.pageNum = pageNum;\r\n            console.log(this.startDate);\r\n            this.$request.get('/article/selectAccPage', {\r\n                params: {\r\n                    pageNum: this.pageNum,\r\n                    pageSize: this.pageSize,\r\n                    author: this.author,\r\n                }\r\n            }).then(res => {\r\n                console.log(res);\r\n                for (var i = 0; i < res.data.list.length; i++) {\r\n\r\n                    var dateTimeString = res.data.list[i].time;\r\n                    var dateTime = new Date(dateTimeString);\r\n                    var year = dateTime.getFullYear();\r\n                    var month = dateTime.getMonth() + 1; // 注意：月份从 0 开始，所以要加 1\r\n                    var day = dateTime.getDate();\r\n                    res.data.list[i].time = year + '-' + (month < 10 ? '0' : '') + month + '-' + (day < 10 ? '0' : '') + day;\r\n                }\r\n                this.tableData = res.data?.list\r\n                this.total = res.data?.total\r\n            })\r\n        },\r\n        loadPro(id) {\r\n            this.$request.get('/user/selectById/' + id).then(res => {\r\n                if (res.code === '200') {\r\n                    this.pro = res.data;\r\n                    console.log(this.pro);\r\n                } else {\r\n                    this.$message.error(res.msg);\r\n                }\r\n            }\r\n            )\r\n\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style>\r\n.category-item {\r\n    text-align: center;\r\n    padding: 10px 0;\r\n    font-size: 16px;\r\n    cursor: pointer;\r\n}\r\n\r\n.category-item-active {\r\n    background-color: #1890ff;\r\n    color: #fff;\r\n    border-radius: 5px;\r\n}\r\n\r\n.search {\r\n    display: flex;\r\n}\r\n\r\n.left {\r\n    flex: 1;\r\n    /* 左侧块会占据剩余空间 */\r\n    //background-color: lightblue;\r\n}\r\n\r\n.right {\r\n    flex: 1;\r\n    /* 右侧块会占据剩余空间 */\r\n    background-color: lightgreen;\r\n}\r\n</style>"],"mappings":"AA8GA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,GAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAN,IAAA;MACAO,OAAA;MACAC,MAAA;MACAC,IAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,QAAA;MAAA;MACAC,SAAA;MACAC,MAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EAEAC,OAAA;IACAC,KAAAjB,OAAA;MACA,SAAAI,MAAA;QACA,KAAAc,QAAA,CAAAC,GAAA,oBACA;UACAC,MAAA;YACAxB,IAAA,OAAAQ,MAAA;YACAiB,IAAA;YACAV,QAAA,OAAAA,QAAA;YACAC,SAAA,OAAAA;UACA;QACA,CACA,EAAAU,IAAA,CAAAC,GAAA;UACA,KAAAC,QAAA,GAAAD,GAAA,CAAA1B,IAAA,EAAA4B,IAAA;UACA,KAAAZ,MAAA,GAAAU,GAAA,CAAA1B,IAAA,EAAAK,KAAA;UAEAwB,OAAA,CAAAC,GAAA,MAAAH,QAAA;QACA;MACA;MACA,IAAAxB,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA0B,OAAA,CAAAC,GAAA,MAAAC,SAAA;MACA,KAAAV,QAAA,CAAAC,GAAA;QACAC,MAAA;UACApB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAG,MAAA,OAAAA;QACA;MACA,GAAAkB,IAAA,CAAAC,GAAA;QACAG,OAAA,CAAAC,GAAA,CAAAJ,GAAA;QACA,SAAAM,CAAA,MAAAA,CAAA,GAAAN,GAAA,CAAA1B,IAAA,CAAA4B,IAAA,CAAAK,MAAA,EAAAD,CAAA;UAEA,IAAAE,cAAA,GAAAR,GAAA,CAAA1B,IAAA,CAAA4B,IAAA,CAAAI,CAAA,EAAAG,IAAA;UACA,IAAAC,QAAA,OAAAC,IAAA,CAAAH,cAAA;UACA,IAAAI,IAAA,GAAAF,QAAA,CAAAG,WAAA;UACA,IAAAC,KAAA,GAAAJ,QAAA,CAAAK,QAAA;UACA,IAAAC,GAAA,GAAAN,QAAA,CAAAO,OAAA;UACAjB,GAAA,CAAA1B,IAAA,CAAA4B,IAAA,CAAAI,CAAA,EAAAG,IAAA,GAAAG,IAAA,UAAAE,KAAA,oBAAAA,KAAA,UAAAE,GAAA,oBAAAA,GAAA;QACA;QACA,KAAAzC,SAAA,GAAAyB,GAAA,CAAA1B,IAAA,EAAA4B,IAAA;QACA,KAAAvB,KAAA,GAAAqB,GAAA,CAAA1B,IAAA,EAAAK,KAAA;MACA;IACA;IA