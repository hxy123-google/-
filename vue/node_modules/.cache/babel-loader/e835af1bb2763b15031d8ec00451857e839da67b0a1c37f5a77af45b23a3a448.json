{"ast":null,"code":"import E from 'wangeditor';\nexport default {\n  data() {\n    let scoreId = this.$route.query.id;\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      scoreId: scoreId,\n      courseData: {},\n      showList: [],\n      pageNum: 1,\n      pageSize: 5,\n      total: 0,\n      fromVisible: null,\n      descr: null,\n      flag: false,\n      editor: null,\n      form: {}\n    };\n  },\n  mounted() {\n    this.loadCourse();\n    this.loadScore();\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    initWangEditor(descr) {\n      this.$nextTick(() => {\n        this.editor = new E('#editor');\n        this.editor.config.placeholder = '请输入内容';\n        this.editor.config.uploadFileName = 'file';\n        this.editor.config.uploadImgServer = 'http://localhost:9090/files/wang/upload';\n        this.editor.create();\n        setTimeout(() => {\n          this.editor.txt.html(descr);\n        });\n      });\n    },\n    loadCourse() {\n      this.$request.get('/Userarticle/selectById/' + this.scoreId).then(res => {\n        if (res.code === '200') {\n          this.courseData = res.data;\n          comsole.log(courseData);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    addDescr() {\n      this.form.descr = this.editor.txt.html();\n      console.log(this.form.descr);\n      this.$request({\n        url: '/Userarticle/edit/',\n        method: 'POST',\n        data: {\n          id: this.scoreId,\n          descr: this.form.descr\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.$message.success('修改成功');\n        } else {\n          this.$message.error(res.msg);\n        }\n        this.loadCourse();\n      });\n    },\n    note(descr) {\n      this.form.descr = JSON.parse(JSON.stringify(descr));\n      this.fromVisible = true;\n      this.initWangEditor(this.form.descr || '');\n      console.log(this.form.descr);\n    },\n    loadScore(pageNum) {\n      console.log(this.courseData);\n      this.pageNum = pageNum;\n      this.$request.get('/articlescore/selectScore/', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          articleId: this.courseData.articleId\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          console.log(res);\n          this.showList = res.data?.list;\n          this.total = res.data?.total;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["E","data","scoreId","$route","query","id","user","JSON","parse","localStorage","getItem","courseData","showList","pageNum","pageSize","total","fromVisible","descr","flag","editor","form","mounted","loadCourse","loadScore","methods","initWangEditor","$nextTick","config","placeholder","uploadFileName","uploadImgServer","create","setTimeout","txt","html","$request","get","then","res","code","comsole","log","$message","error","msg","addDescr","console","url","method","success","note","stringify","params","articleId","list"],"sources":["src/views/front/ruser.vue"],"sourcesContent":["<template>\r\n    <div class=\"main-content\">\r\n        <div style=\"display: flex; grid-gap:10px\">\r\n            <div style=\"flex:1 ;width: 50%; margin: 20px auto; min-height: 1000px\">\r\n                <div style=\"text-align: center; margin-top: 15px\">\r\n                    <span style=\"font-size: 20px; font-weight: 550; color: #333333; margin-left: 20px\">文献名称：{{ courseData.articlename }}</span>\r\n                </div>\r\n                <div style=\"text-align: center; margin-top: 15px\">\r\n                    <span style=\"color: #666666; margin-left: 50px\">文献作者：{{ courseData.author }}</span>\r\n                </div>\r\n                <div style=\"text-align: center\">\r\n                    <el-button type=\"success\">{{ courseData.type === 'ENGLISH' ? '英文' : '中文' }}</el-button>\r\n                    <span style=\"font-size: 20px; font-weight: 550; color: #333333; margin-left: 20px\">{{\r\n                        courseData.name\r\n                    }}</span>\r\n                </div>\r\n                <div style=\"text-align: center; margin-top: 15px\">\r\n                    <span style=\"color: #666666; margin-left: 50px\">发布时间：{{ courseData.time }}</span>\r\n                </div>\r\n                <!--   课程保密区域   -->\r\n                <div>\r\n                    <div style=\"font-size: 18px; margin: 10px 0\">资料</div>\r\n                    <div style=\"margin-top: 10px\">文献链接：<a :href=\"courseData.file\" target=\"_blank\">{{ courseData.file\r\n                            }}</a>\r\n                    </div>\r\n                </div>\r\n                <!--   课程介绍区域   -->\r\n                <div style=\"margin-top: 20px\">\r\n                    <div style=\"font-size: 18px\">文献笔记</div>\r\n                    <el-button type=\"info\" plain style=\"margin-left: 10px\" size=\"mini\"\r\n                        @click=\"note(courseData.descr)\">增加笔记</el-button>\r\n                    <div style=\"margin-top: 10px\" v-html=\"courseData.descr\" class=\"w-e-text w-e-text-container\"></div>\r\n                </div>\r\n            </div>\r\n            <el-dialog title=\"添加笔记\" :visible.sync=\"fromVisible\" width=\"55%\" :close-on-click-modal=\"false\"\r\n                destroy-on-close>\r\n                <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n                    <el-form-item prop=\"descr\" label=\"文献笔记\">\r\n                        <div id=\"editor\"></div>\r\n                    </el-form-item>\r\n                </el-form>\r\n                <div slot=\"footer\" class=\"dialog-footer\">\r\n                    <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n                    <el-button type=\"primary\" @click=\"addDescr\">确 定</el-button>\r\n                </div>\r\n            </el-dialog>\r\n            <div style=\"width:260px\" class=\"card\">\r\n                <div style=\"display: flex; align-items: center; padding-bottom: 10px; border-bottom: 1px solid #ddd\">\r\n                    <div style=\"font-size: 20px; flex: 1\">文章榜单</div>\r\n                </div>\r\n                <div>\r\n                    <div v-for=\"item in showList\" :key=\"item.id\" style=\"margin: 15px 0\" class=\"line1\">\r\n                        <span style=\"width: 18px; display: inline-block; text-align: right; margin-right: 10px\">\r\n                            <span style=\"color: orangered\" v-if=\"item.index === 1\">{{ item.index }}</span>\r\n                            <span style=\"color: goldenrod\" v-else-if=\"item.index === 2\">{{ item.index }}</span>\r\n                            <span style=\"color: dodgerblue\" v-else-if=\"item.index === 3\">{{ item.index }}</span>\r\n                            <span style=\"color: #666\" v-else>{{ item.index }}</span>\r\n                        </span>\r\n                        <span style=\"color: #666;\"> <a :href=\"'/front/ArticleDetail?id=' + item.id\">{{ item.name\r\n                                }}</a></span>\r\n                    </div>\r\n                </div>\r\n                <div class=\"pagination\">\r\n                    <el-pagination background @current-change=\"handleCurrentChange\" :current-page=\"pageNum\"\r\n                        :page-sizes=\"[5, 10, 20]\" :page-size=\"pageSize\" layout=\"total, prev, pager, next\"\r\n                        :total=\"total\">\r\n                    </el-pagination>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport E from 'wangeditor'\r\nexport default {\r\n\r\n    data() {\r\n        let scoreId = this.$route.query.id\r\n        return {\r\n            user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n            scoreId: scoreId,\r\n            courseData: {},\r\n            showList: [],\r\n            pageNum: 1,\r\n            pageSize: 5,\r\n            total: 0,\r\n            fromVisible: null,\r\n            descr: null,\r\n            flag: false,\r\n            editor: null,\r\n            form: {},\r\n        }\r\n    },\r\n    mounted() {\r\n        this.loadCourse();\r\n        this.loadScore();\r\n    },\r\n    // methods：本页面所有的点击事件或者其他函数定义区\r\n    methods: {\r\n        initWangEditor(descr) {\r\n            this.$nextTick(() => {\r\n                this.editor = new E('#editor')\r\n                this.editor.config.placeholder = '请输入内容'\r\n                this.editor.config.uploadFileName = 'file'\r\n                this.editor.config.uploadImgServer = 'http://localhost:9090/files/wang/upload'\r\n                this.editor.create()\r\n                setTimeout(() => {\r\n                    this.editor.txt.html(descr)\r\n                })\r\n            })\r\n        },\r\n        loadCourse() {\r\n            this.$request.get('/Userarticle/selectById/' + this.scoreId).then(res => {\r\n                if (res.code === '200') {\r\n                    this.courseData = res.data;\r\n                    comsole.log(courseData);\r\n                } else {\r\n                    this.$message.error(res.msg)\r\n                }\r\n            })\r\n        },\r\n        addDescr() {\r\n            this.form.descr = this.editor.txt.html();\r\n            console.log(this.form.descr);\r\n            this.$request({\r\n                url:'/Userarticle/edit/', \r\n                method:'POST',\r\n                data: {\r\n                    id: this.scoreId,\r\n                    descr: this.form.descr\r\n                }\r\n            }).then(res => {\r\n                if (res.code === '200') {\r\n                    this.$message.success('修改成功')\r\n\r\n                } else {\r\n                    this.$message.error(res.msg)\r\n                }\r\n                this.loadCourse();\r\n            })\r\n\r\n        },\r\n        note(descr) {\r\n            this.form.descr = JSON.parse(JSON.stringify(descr))\r\n            this.fromVisible = true;\r\n            this.initWangEditor(this.form.descr || '')\r\n            console.log(this.form.descr);\r\n        },\r\n        loadScore(pageNum) {\r\n            console.log(this.courseData);\r\n            this.pageNum=pageNum;\r\n            this.$request.get('/articlescore/selectScore/', {\r\n                params: {\r\n                    pageNum: this.pageNum,\r\n                    pageSize: this.pageSize,\r\n                    articleId: this.courseData.articleId\r\n                }\r\n            }).then(res => {\r\n                if (res.code === '200') {\r\n                    console.log(res);\r\n                    this.showList = res.data?.list\r\n                    this.total = res.data?.total\r\n                }\r\n            })\r\n        },\r\n    }\r\n}\r\n</script>\r\n<style scoped></style>"],"mappings":"AAyEA,OAAAA,CAAA;AACA;EAEAC,KAAA;IACA,IAAAC,OAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,EAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAR,OAAA,EAAAA,OAAA;MACAS,UAAA;MACAC,QAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,WAAA;MACAC,KAAA;MACAC,IAAA;MACAC,MAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,UAAA;IACA,KAAAC,SAAA;EACA;EACA;EACAC,OAAA;IACAC,eAAAR,KAAA;MACA,KAAAS,SAAA;QACA,KAAAP,MAAA,OAAAnB,CAAA;QACA,KAAAmB,MAAA,CAAAQ,MAAA,CAAAC,WAAA;QACA,KAAAT,MAAA,CAAAQ,MAAA,CAAAE,cAAA;QACA,KAAAV,MAAA,CAAAQ,MAAA,CAAAG,eAAA;QACA,KAAAX,MAAA,CAAAY,MAAA;QACAC,UAAA;UACA,KAAAb,MAAA,CAAAc,GAAA,CAAAC,IAAA,CAAAjB,KAAA;QACA;MACA;IACA;IACAK,WAAA;MACA,KAAAa,QAAA,CAAAC,GAAA,mCAAAlC,OAAA,EAAAmC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAA5B,UAAA,GAAA2B,GAAA,CAAArC,IAAA;UACAuC,OAAA,CAAAC,GAAA,CAAA9B,UAAA;QACA;UACA,KAAA+B,QAAA,CAAAC,KAAA,CAAAL,GAAA,CAAAM,GAAA;QACA;MACA;IACA;IACAC,SAAA;MACA,KAAAzB,IAAA,CAAAH,KAAA,QAAAE,MAAA,CAAAc,GAAA,CAAAC,IAAA;MACAY,OAAA,CAAAL,GAAA,MAAArB,IAAA,CAAAH,KAAA;MACA,KAAAkB,QAAA;QACAY,GAAA;QACAC,MAAA;QACA/C,IAAA;UACAI,EA