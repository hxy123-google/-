{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport E from 'wangeditor';\nexport default {\n  name: \"FrontLayout\",\n  data() {\n    return {\n      editor: null,\n      top: '',\n      notice: [],\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || '{}'),\n      form: {}\n    };\n  },\n  mounted() {\n    this.loadNotice();\n  },\n  methods: {\n    initWangEditor(content) {\n      this.$nextTick(() => {\n        this.editor = new E('#editor');\n        this.editor.config.placeholder = '请输入内容';\n        this.editor.config.uploadFileName = 'file';\n        this.editor.config.uploadImgServer = 'http://localhost:9090/files/wang/upload';\n        this.editor.create();\n        setTimeout(() => {\n          this.editor.txt.html(content);\n        });\n      });\n    },\n    loadNotice() {\n      this.$request.get('/notice/selectAll').then(res => {\n        this.notice = res.data;\n        let i = 0;\n        if (this.notice && this.notice.length) {\n          this.top = this.notice[0].content;\n          setInterval(() => {\n            this.top = this.notice[i].content;\n            i++;\n            if (i === this.notice.length) {\n              i = 0;\n            }\n          }, 2500);\n        }\n      });\n    },\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}'); // 重新获取下用户的最新信息\n    },\n\n    // 退出登录\n    logout() {\n      localStorage.removeItem(\"xm-user\");\n      this.$router.push(\"/login\");\n    },\n    navto(url) {\n      location.href = url;\n    }\n  }\n};","map":{"version":3,"names":["E","name","data","editor","top","notice","user","JSON","parse","localStorage","getItem","form","mounted","loadNotice","methods","initWangEditor","content","$nextTick","config","placeholder","uploadFileName","uploadImgServer","create","setTimeout","txt","html","$request","get","then","res","i","length","setInterval","updateUser","logout","removeItem","$router","push","navto","url","location","href"],"sources":["src/views/Front.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"front-notice\"><i class=\"el-icon-bell\" style=\"margin-right: 2px\"></i>公告：{{ top }}</div>\r\n    <!--头部-->\r\n    <div class=\"front-header\">\r\n      <div class=\"front-header-left\">\r\n        <img src=\"@/assets/imgs/logo.png\" alt=\"\">\r\n        <div class=\"title\">材料科学文献管理系统</div>\r\n      </div>\r\n      <div class=\"front-header-center\">\r\n        <div class=\"front-header-nav\">\r\n          <el-menu :default-active=\"$route.path\" mode=\"horizontal\" router>\r\n            <el-menu-item index=\"/front/home\">首页</el-menu-item>\r\n            <el-menu-item>我的收藏</el-menu-item>\r\n            <el-menu-item index=\"/front/article\">文献搜索</el-menu-item>\r\n            <el-menu-item index=\"/front/score\">积分专区</el-menu-item>\r\n            <el-menu-item>会员专区</el-menu-item>\r\n            <el-menu-item>文献上传</el-menu-item>\r\n          </el-menu>\r\n        </div>\r\n      </div>\r\n      <div class=\"front-header-right\">\r\n        <div v-if=\"!user.username\">\r\n          <el-button @click=\"$router.push('/login')\">登录</el-button>\r\n          <el-button @click=\"$router.push('/register')\">注册</el-button>\r\n        </div>\r\n        <div v-else>\r\n          <el-dropdown>\r\n            <div class=\"front-header-dropdown\">\r\n              <img :src=\"user.avatar\" alt=\"\">\r\n              <div style=\"margin-left: 10px\">\r\n                <span>{{ user.name }}</span><i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\r\n              </div>\r\n            </div>\r\n            <el-dropdown-menu slot=\"dropdown\">\r\n              <el-dropdown-item>\r\n                <div style=\"text-decoration: none\" @click=\"logout\">退出</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div style=\"text-decoration: none\" @click=\"navto('/front/person')\">个人中心</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div style=\"text-decoration: none\" @click=\"navto('/front/scoreOrder')\">我的资料</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div style=\"text-decoration: none\" @click=\"navto('/front/ImSingle')\">私信</div>\r\n              </el-dropdown-item>\r\n              <el-dropdown-item>\r\n                <div style=\"text-decoration: none\" @click=\"navto('/front/UploadScore')\">上传资料</div>\r\n              </el-dropdown-item>\r\n            </el-dropdown-menu>\r\n          </el-dropdown>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!--主体-->\r\n    <div class=\"main-body\">\r\n      <router-view ref=\"child\" @update:user=\"updateUser\" />\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport E from 'wangeditor'\r\nexport default {\r\n  name: \"FrontLayout\",\r\n\r\n  data() {\r\n    return {\r\n      editor: null,\r\n      top: '',\r\n      notice: [],\r\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || '{}'),\r\n      form: {},\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.loadNotice()\r\n  },\r\n  methods: {\r\n    initWangEditor(content) {\r\n      this.$nextTick(() => {\r\n        this.editor = new E('#editor')\r\n        this.editor.config.placeholder = '请输入内容'\r\n        this.editor.config.uploadFileName = 'file'\r\n        this.editor.config.uploadImgServer = 'http://localhost:9090/files/wang/upload'\r\n        this.editor.create()\r\n        setTimeout(() => {\r\n          this.editor.txt.html(content)\r\n        })\r\n      })\r\n    },\r\n    loadNotice() {\r\n      this.$request.get('/notice/selectAll').then(res => {\r\n        this.notice = res.data\r\n        let i = 0\r\n        if (this.notice && this.notice.length) {\r\n          this.top = this.notice[0].content\r\n          setInterval(() => {\r\n            this.top = this.notice[i].content\r\n            i++\r\n            if (i === this.notice.length) {\r\n              i = 0\r\n            }\r\n          }, 2500)\r\n        }\r\n      })\r\n    },\r\n    updateUser() {\r\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}')   // 重新获取下用户的最新信息\r\n    },\r\n    // 退出登录\r\n    logout() {\r\n      localStorage.removeItem(\"xm-user\");\r\n      this.$router.push(\"/login\");\r\n    },\r\n    navto(url) {\r\n      location.href = url;\r\n    },\r\n\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import \"@/assets/css/front.css\";\r\n</style>"],"mappings":";AAgEA,OAAAA,CAAA;AACA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,MAAA;MACAC,GAAA;MACAC,MAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,IAAA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,UAAA;EACA;EACAC,OAAA;IACAC,eAAAC,OAAA;MACA,KAAAC,SAAA;QACA,KAAAd,MAAA,OAAAH,CAAA;QACA,KAAAG,MAAA,CAAAe,MAAA,CAAAC,WAAA;QACA,KAAAhB,MAAA,CAAAe,MAAA,CAAAE,cAAA;QACA,KAAAjB,MAAA,CAAAe,MAAA,CAAAG,eAAA;QACA,KAAAlB,MAAA,CAAAmB,MAAA;QACAC,UAAA;UACA,KAAApB,MAAA,CAAAqB,GAAA,CAAAC,IAAA,CAAAT,OAAA;QACA;MACA;IACA;IACAH,WAAA;MACA,KAAAa,QAAA,CAAAC,GAAA,sBAAAC,IAAA,CAAAC,GAAA;QACA,KAAAxB,MAAA,GAAAwB,GAAA,CAAA3B,IAAA;QACA,IAAA4B,CAAA;QACA,SAAAzB,MAAA,SAAAA,MAAA,CAAA0B,MAAA;UACA,KAAA3B,GAAA,QAAAC,MAAA,IAAAW,OAAA;UACAgB,WAAA;YACA,KAAA5B,GAAA,QAAAC,MAAA,CAAAyB,CAAA,EAAAd,OAAA;YACAc,CAAA;YACA,IAAAA,CAAA,UAAAzB,MAAA,CAAA0B,MAAA;cACAD,CAAA;YACA;UACA;QACA;MACA;IACA;IACAG,WAAA;MACA,KAAA3B,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;;IACA;IACAwB,OAAA;MACAzB,YAAA,CAAA0B,UAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,MAAAC,GAAA;MACAC,QAAA,CAAAC,IAAA,GAAAF,GAAA;IACA;EAEA;AAEA"},"metadata":{},"sourceType":"module","externalDependencies":[]}