<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.ImSingleMapper">

    <insert id="insertSelective" parameterType="ImSingle" useGeneratedKeys="true" keyProperty="id">
        insert into imsingle
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="content != null">content,</if>
            <if test="fromuser != null">fromuser,</if>
            <if test="fromavatar != null">fromavatar,</if>
            <if test="time != null">time,</if>
            <if test="type != null">type,</if>
            <if test="touser != null">touser,</if>
            <if test="toavatar != null">toavatar,</if>
            <if test="readed != null">readed</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="content != null">#{content},</if>
            <if test="fromuser != null">#{fromuser},</if>
            <if test="fromavatar != null">#{fromavatar},</if>
            <if test="time != null">#{time},</if>
            <if test="type != null">#{type},</if>
            <if test="touser != null">#{touser},</if>
            <if test="toavatar != null">#{toavatar},</if>
            <if test="readed != null">#{readed}</if>
        </trim>
    </insert>



</mapper>